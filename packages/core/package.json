    "types-check": "tsc --noemit"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@types/uuid": "^9.0.8",
    "tsup": "^8.0.1",
    "typescript": "^5.4.2",
    "zod": "^3.22.4"
  },
  "dependencies": {
    "pipe-ts": "^0.0.9",
    "uuid": "^9.0.1"
  },
  "peerDependencies": {
    "zod": "^3.22.4"
  }
