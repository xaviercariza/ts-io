{
  "name": "@tsio/tests",
  "description": "Tests suite for tsio",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "prettier": "prettier --write src/**/*.ts --config ../../prettier.config.cjs",
    "types-check": "tsc --noemit",
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@tsio/core": "workspace:^",
    "@tsio/eslint-config": "workspace:*",
    "@tsio/socketio": "workspace:^",
    "@tsio/typescript-config": "workspace:*",
    "@tsio/ws": "workspace:^",
    "@types/node": "20.10.0",
    "@types/ws": "^8.5.10",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4",
    "typescript": "^5.4.2",
    "vitest": "^1.3.1",
    "vitest-mock-extended": "^1.3.1",
    "ws": "^8.16.0",
    "zod": "^3.22.4"
  }
}
